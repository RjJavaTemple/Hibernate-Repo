<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="Com.Rj.Entity.SetAndGetImp" table="Actor" >

<id name="SNO" type="integer" length="15">
<generator class="increment"/>
</id>

<property name="NAME" type="string"  length="15"/>
<property name="ADDRESS" type="string"  length="15"/>
<property name="PHOTONAME" type="string" length="15"/>

</class>
</hibernate-mapping>